<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
    <key>Description</key>
    <string>Imports the latest version of Bambu Studio into Munki.</string>
    <key>Identifier</key>
    <string>com.github.bmeehankas.munki.BambuStudio</string>
    <key>Input</key>
    <dict>
        <key>MUNKI_REPO_SUBDIR</key>
        <string>apps</string>
        <key>NAME</key>
        <string>Bambu Studio</string>
        <key>pkginfo</key>
        <dict>
            <key>category</key>
            <string>STEM</string>
            <key>developer</key>
            <string>Bambu Lab</string>
            <key>display_name</key>
            <string>Bambu Studio</string>
            <key>description</key>
            <string>Bambu Studio is an open-source, cutting-edge, feature-rich slicing software. It contains project-based workflows, systematically optimized slicing algorithms, and an easy-to-use graphical interface, bringing users an incredibly smooth printing experience.</string>
            <key>unattended_install</key>
            <true/>
            <key>blocking_applications</key>
            <array>
                <string>Bambu Studio.app</string>
            </array>
        </dict>
    </dict>
    <key>ParentRecipe</key>
    <string>com.github.bmeehankas.download.BambuStudio</string>
    <key>MinimumVersion</key>
    <string>1.0.0</string>
    <key>Process</key>
    <array>
        <dict>
            <key>Processor</key>
            <string>AppDmgVersioner</string>
            <key>Arguments</key>
            <dict>
                <key>dmg_path</key>
                <string>%pathname%</string>
                <key>app_path</key>
                <string>/Applications/Bambu Studio.app</string>
            </dict>
        </dict>
        <dict>
            <key>Processor</key>
            <string>MunkiImporter</string>
            <key>Arguments</key>
            <dict>
                <key>pkg_path</key>
                <string>%pathname%</string>
                <key>repo_subdirectory</key>
                <string>%MUNKI_REPO_SUBDIR%</string>
            </dict>
        </dict>
        <dict>
            <key>Processor</key>
            <string>UnmountDiskImage</string>
        </dict>
    </array>
</dict>
</plist>